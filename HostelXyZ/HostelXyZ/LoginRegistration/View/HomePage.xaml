<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="HostelXyZ.LoginRegistration.View.HomePage">
    <ContentPage.Content>
        <StackLayout>
            <Button
                    x:Name= "loginbtn" Text = "Log Out "
                    Command="{Binding Loginbtn_Clicked}"
                    Padding="15"
                    FontSize="16"
                    FontAttributes="Bold"
                    TextColor="WhiteSmoke"
                    BackgroundColor="Red"
                    BorderColor="Black"
                    BorderWidth="1"
                    CornerRadius="10"
                    WidthRequest="200"
                ></Button>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>